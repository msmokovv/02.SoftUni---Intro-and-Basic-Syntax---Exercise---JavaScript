function division([number]) {
    let n = Number(number);
    let devider = 0;
    
    if(n % 2 == 0){
        devider = 2;
    }
    if(n % 3 == 0){
        devider = 3;
    }
    if(n % 6 == 0){
        devider = 6;
    }
    if(n % 7 == 0){
        devider = 7;
    } 
    if(n % 10 == 0){
        devider = 10;
    }

    if (devider == 0)
    {
        console.log("Not devisible");
    }
    else
    {
        console.log("The number is divisible by " + devider);
    }
}
division([1643])
